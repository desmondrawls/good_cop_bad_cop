<%= form_tag(root_path, :method => "get") do %>
  <%= text_field_tag :badge_number, params[:badge_number], placeholder: "Badge Number" %>
  <%= submit_tag "Search" %>
<% end %>

<ul class="cops">
  <% if @cops %>
    <% @cops.each do |cop| %>
    <li><%= cop.name %></li>
    <% end %>
  <% end %>
</ul>   